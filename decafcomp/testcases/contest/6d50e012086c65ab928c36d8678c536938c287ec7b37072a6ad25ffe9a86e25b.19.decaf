extern func print_int(int) void;
extern func print_string(string) void;

package SpecialChars {
    func main() int {
        // String has ASCII codes 7, 8, 9, 11, 12, 13
        print_string("This is a string with some special chars: 	\n");

        print_int(''); print_string("\n"); // ASCII code 7: 
        print_int(''); print_string("\n"); // ASCII code 8: 
        print_int('	'); print_string("\n"); // ASCII code 9: 	
        // ASCII code 10 tested in charlitnewline
        print_int(''); print_string("\n"); // ASCII code 11: 
        print_int(''); print_string("\n"); // ASCII code 12: 
        print_int(''); print_string("\n"); // ASCII code 13: 
    }
}
