extern func print_string(string) void;
extern func print_int(int) void;

package Factorial {

	func factorial(n int) void {
    var i int;
    var fact int;
    fact = 1;

    for (i = n; i > 0; i = i - 1) {
      fact = fact * i;
    }

    print_string("Factorial of ");
    print_int(n);
    print_string(" is ");
    print_int(fact);
    print_string("\n");
	}

	func main() int {
    var a int;
    var b int;

    a = 5;
    b = 10;

		factorial(a);
    factorial(b);

		return(0);
	}
}
