extern func print_int(int) void;
extern func print_string(string) void;

package fib {
    func main() int {
        var x int;
        x = fib(10);
        print_string("The 10th fibonacci number is: ");
        print_int(x);
        print_string("\n");
    }

    func fib(n int) int {
        if (n <= 0) { return(0); }
        if (n == 1) { return(1); }
        return(fib(n-1) + fib(n-2));
    }
}
