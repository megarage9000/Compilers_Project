extern func print_int(int) void;
extern func print_string(string) void;

package loop {
    func countDown(time int) bool {
        if (time == 0) {
            return (false);
        } else {
            return (countDown(time - 1));
        }
    }

    func main() int {
        var flag bool;
        var x int;

        x = 5;
        flag = true;
        while(countDown(x) && flag) {
            print_string("Oh no\n");
        }
        print_string("Blastoff!");
    }
}