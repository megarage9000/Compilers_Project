extern func print_string(string) void;
extern func print_int(int) void;

package SumOfCubesOfN {

	func sumOfCubeOfN(n int) void {
    var res int;
    res = n * n * (n + 1) * (n + 1) / 4;

    print_string("Sum of cubes of first ");
    print_int(n);
    print_string(" natural numbers is ");
    print_int(res);
    print_string("\n");
	}

	func main() int {
    var a int;
    var b int;

    a = 5;
    b = 10;

		sumOfCubeOfN(a);
    sumOfCubeOfN(b);

		return(0);
	}
}
