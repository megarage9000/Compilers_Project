extern func print_int(int) void;

package Fibonnaci {

    func fibonacci(n int) int {
        if (n == 0 || n == 1) {
            return (n);
        }

        return (fibonacci(n - 1) + fibonacci(n - 2));
    }

    func main() int {
        var f int;
        f = fibonacci(11);

        print_int(f);
    }
}