extern func print_string(string) void;
extern func print_int(int) void;

package SumOfN {

	func sumOfN(n int) void {
    var res int;
    res = n * (n + 1) / 2;

    print_string("Sum of first ");
    print_int(n);
    print_string(" natural numbers is ");
    print_int(res);
    print_string("\n");
	}

	func main() int {
    var a int;
    var b int;

    a = 5;
    b = 10;

		sumOfN(a);
    sumOfN(b);

		return(0);
	}
}
