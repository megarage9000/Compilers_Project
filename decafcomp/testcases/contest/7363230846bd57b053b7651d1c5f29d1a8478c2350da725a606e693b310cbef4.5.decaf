extern func print_int(int) void;
extern func print_string(string) void;

// This test case defines an extern function with the same name as a field.
// Compilers that do not use the symbol table correctly will fail
extern func x(int) void;

package DecafDestroyer {
    var x int;

    func main() int {
        x = 1;

        print_string("4: extern_shadow_field\n");
    }
}