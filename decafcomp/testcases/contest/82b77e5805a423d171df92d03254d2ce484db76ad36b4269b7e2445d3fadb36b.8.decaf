extern func print_int(int) void;
package Test {
    func main() int
    {
        var x int;
        x = 0;
        while (x<7) {
            x = x + 1;
            print_int(x);
            if (x > 3){
                continue;
            }
            if (x == 5){
                break;
            }
        }
    }
}
