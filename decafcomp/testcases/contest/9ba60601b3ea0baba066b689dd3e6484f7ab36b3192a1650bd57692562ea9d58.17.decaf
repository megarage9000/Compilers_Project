extern func print_string(string) void;

package whileontour {
    func main() int {
        var i int;
        for (i=0;i<5;i=i+1) {
            var j int;
            while (j == i) {
                print_string("are you ok? ");
                break;
            }
            print_string("ok\n");
        }
    }
}
