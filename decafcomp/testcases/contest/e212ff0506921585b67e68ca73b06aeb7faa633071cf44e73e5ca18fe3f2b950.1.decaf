extern func print_string(string) void;
extern func print_int(int) void;

package AddToEachElem {

	var list [100]int;

	func displayList(start int, len int) void {
		var j int;
		print_string("List:\n");
		for (j = start; j < start + len; j = j + 1) {
			print_int(list[j]);
			print_string(" ");
			if ((j - start + 1) % 20 == 0 || j + 1 == start + len) {
				print_string("\n");
			}
		}
	}

	func initList(size int) void {
		var i int;
		for (i = 0; i < size; i = i + 1) {
			list[i] = (i * 2382983) % 100;
		}
	}

	func addToEachElem(increment int, size int) void {
    var i int;
    for (i = 0; i < size; i = i + 1) {
      list[i] = list[i] + increment;
    }
	}

	func main() int {
		var size int;
		size = 100;
		initList(size);
		displayList(0,size);

		addToEachElem(10, size);
		displayList(0,size);
		return(0);
	}
}
