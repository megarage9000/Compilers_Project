extern func print_int(int) void;
extern func print_string(string) void;

package ExternScope {
    func print_int(x int) void{
        if(x == 5) {
            print_string("Printing 5....");
        } else {
            print_string("Printing another number");
        }
    }
    func main() int {
        print_int(5);
        print_int(8);
    }
}